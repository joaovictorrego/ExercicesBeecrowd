#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void limparBuffer() {
  int c;
  while ((c = getchar()) != '\n' && c != EOF) {
    // Descartar caracteres
  }
}

int main(){
  int N;
  int i, j, k;
  
  int umEspaco;
  
  scanf("%d", &N);
  
  while(N > 0){
    limparBuffer();
    palavras = (char **) malloc(sizeof(char *) * N);
    
    for(i = 0; i < N; i++){
      palavras[i] = (char *) malloc(50 * sizeof(char));
    }

    for(i = 0; i < N; i++){
      fgets(palavras[i], 50, stdin);
      // palavras[i][strcspn(palavras[i], "\n")] = '\0';
    }

    for(i = 0; i < N; i++){
      umEspaco = 0;
      char aux[50] = "";
      k = 0;
      
      for(j = 0; j < strlen(palavras[i]); j++){
	if(palavras[i][j] != 32)
      }
      
    }
    
  }

  return 0;
}
